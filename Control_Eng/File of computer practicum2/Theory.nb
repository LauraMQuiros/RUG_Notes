Cell["Exercise 4", "Section",
 CellChangeTimes->{{3.925800344576482*^9, 
  3.925800346776125*^9}},ExpressionUUID->"fb3c1ca0-2e2b-4f76-981e-\
e3405156f72a"],

Cell[TextData[{
 "There are two ways to get the state space form of the new equations of \
motion, the inverse Laplace transform and the original procedure we did in \
the last computer lab. \nThe new equations of motion are ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          OverscriptBox[
           StyleBox["q", "TI"], "\:0308"], 
          StyleBox["m", "TI"]], "\[LongEqual]", 
         FractionBox[
          StyleBox["K", "TI"], 
          RowBox[{
            StyleBox["J", "TI"], 
            StyleBox["t", "TI"]}]], 
         SubscriptBox[
          OverscriptBox[
           StyleBox["q", "TI"], "."], 
          StyleBox["e", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\ddot{q}_m = \\frac{K}{Jt} \\dot{q}_e", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "c60276dc-f197-49a2-a8a0-5d8f1161acf2"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"0", "\[LongEqual]", "-", 
         StyleBox["k", "TI"], 
         SubscriptBox[
          OverscriptBox[
           StyleBox["q", "TI"], "."], 
          StyleBox["m", "TI"]], "-", 
         StyleBox["R", "TI"], 
         SubscriptBox[
          OverscriptBox[
           StyleBox["q", "TI"], "."], 
          StyleBox["e", "TI"]], "+", 
         StyleBox["u", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "0 = -k\\dot{q}_m - R\\dot{q}_e +u", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ad54e9e9-ac18-4468-b608-e01e0c78c84b"],
 " which can be rewritten as ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          OverscriptBox[
           StyleBox["q", "TI"], "."], 
          StyleBox["e", "TI"]], "\[LongEqual]", 
         RowBox[{"(", 
           RowBox[{
             StyleBox["u", "TI"], "-", 
             StyleBox["K", "TI"], 
             SubscriptBox[
              OverscriptBox[
               StyleBox["q", "TI"], "."], 
              StyleBox["m", "TI"]]}], ")"}], "/", 
         StyleBox["R", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\dot{q}_e = (u - K\\dot{q}_m)/R", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "d90d6b01-8bad-4fc3-89df-7b5e726adc95"],
 "\nand the state space form is ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["x", "TI"], "\[LongEqual]", 
         RowBox[{"(", 
           RowBox[{
             StyleBox["q", "TI"], 
             StyleBox["m", "TI"], ",", 
             OverscriptBox[
              StyleBox["q", "TI"], "."], 
             StyleBox["m", "TI"]}], ")"}]}], TraditionalForm], "errors" -> {},
      "input" -> "x= (qm, \\dot{q}m)", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "8321b06a-307f-40c2-9e82-df46d20b1bfe"],
 "\nthat means that\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          OverscriptBox[
           StyleBox["x", "TI"], "."], "1"], "\[LongEqual]", 
         SubscriptBox[
          StyleBox["x", "TI"], "2"], "\n", 
         SubscriptBox[
          OverscriptBox[
           StyleBox["x", "TI"], "."], "2"], "\[LongEqual]", "-", 
         FractionBox[
          SuperscriptBox[
           StyleBox["K", "TI"], "2"], 
          RowBox[{
            SubscriptBox[
             StyleBox["J", "TI"], 
             StyleBox["t", "TI"]], 
            StyleBox["R", "TI"]}]], 
         SubscriptBox[
          StyleBox["x", "TI"], "2"], "+", 
         FractionBox[
          StyleBox["K", "TI"], 
          RowBox[{
            SubscriptBox[
             StyleBox["J", "TI"], 
             StyleBox["t", "TI"]], 
            StyleBox["R", "TI"]}]], 
         StyleBox["u", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\dot{x}_1 = x_2 \\\\\n\\dot{x}_2 = -\\frac{K^2}{J_t R} x_2 + \
\\frac{K}{J_t R} u", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "35d431e6-a0eb-4e03-982b-ec34c262eab2"]
}], "Text",
 CellChangeTimes->{{3.925800372725482*^9, 3.925800430673256*^9}, {
