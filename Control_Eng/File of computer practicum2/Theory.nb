Cell[CellGroupData[{

Cell["Exercise 2", "Section",
 CellChangeTimes->{{3.925718097251396*^9, 
  3.925718099276598*^9}},ExpressionUUID->"76dd46a6-49cc-4dd4-bbd6-\
a7e40bd71e39"],

Cell[TextData[{
 "Because eigenvalues for a standard second order system controller have the \
form ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Lambda]", "\[LongEqual]", "-", 
         SubscriptBox["\[Xi]\[Omega]", "0"], "\[PlusMinus]", 
         SqrtBox[
          RowBox[{
            SubsuperscriptBox["\[Omega]", "0", "2"], "(", 
            RowBox[{
              SuperscriptBox["\[Xi]", "2"], "-", "1"}], ")"}]], ","}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\lambda=-\\xi\\omega_0 \\pm \\sqrt{ \\omega_0^2 ( \\xi^2-1 )},", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cccd6104-8f33-4883-9a0c-397523e022de"],
 " according to the textbook section 6.3 we can say that ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Lambda]", "\[LongEqual]", "-100", "\[PlusMinus]", "100"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\lambda = -100 \\pm 100", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "1949a5fd-293b-41d0-ab70-b4acb5e42df1"],
 " means that ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Xi]\[Omega]", "0"], "\[LongEqual]", "100"}], 
       TraditionalForm], "errors" -> {}, "input" -> "\\xi\\omega_0 = 100", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "c4255fab-141c-407d-9c66-045ba0c65e51"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SqrtBox[
          RowBox[{
            SubsuperscriptBox["\[Omega]", "0", "2"], "(", 
            RowBox[{
              SuperscriptBox["\[Xi]", "2"], "-", "1"}], ")"}]], 
         "\[LongEqual]", "100"}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\sqrt{ \\omega_0^2 ( \\xi^2-1 )} = 100", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "c02537d9-918e-48d9-a5ec-e15190891cfb"],
 ".\nBecause we know the natural frequency is the magnitude of the real and \
imaginary parts we can say that ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Omega]", "0"], "\[LongEqual]", 
         SqrtBox[
          RowBox[{
            SuperscriptBox[
             RowBox[{"(", "-100", ")"}], "2"], "+", 
            SuperscriptBox["100", "2"]}]], "\[LongEqual]", "100", 
         SqrtBox["2"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\omega_0 = \\sqrt{(-100)^2 + 100^2}= 100 \\sqrt{2}", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "8f77a5b2-fe73-4c23-a277-54828daecec5"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Xi]", "\[LongEqual]", 
         FractionBox["100", 
          SubscriptBox["\[Omega]", "0"]], "\[LongEqual]", 
         FractionBox["1", 
          SqrtBox["2"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\xi = \\frac{100}{ \\omega_0} = \\frac{1}{\\sqrt{2}}", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f8f84efd-e60f-456a-bf9a-0222e2f1973b"],
 "\nNow rise time ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["t", "TI"], 
          StyleBox["r", "TI"]], "\[LongEqual]", 
         FractionBox["1", 
          SubscriptBox["\[Omega]", "0"]], "\[CenterDot]", 
         FractionBox[
          SuperscriptBox[
           StyleBox["e", "TI"], "\[Phi]"], 
          RowBox[{"tan", "(", "\[Phi]", ")"}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "t_r =  \\frac{1}{\\omega_0} \[CenterDot]\\frac{e^\\phi}{\\tan(\\phi)}", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "4d440cc8-558f-4830-a9fd-f28c2cec414a"],
 "  where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Phi]", "\[LongEqual]", "arccos", 
         RowBox[{"(", "\[Xi]", ")"}]}], TraditionalForm], "errors" -> {}, 
     "input" -> "\\phi  = \\arccos (\\xi) ", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cf9301ed-02f2-429f-98ea-745a63a4e2cf"],
 ".\nsettling time ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["t", "TI"], 
          StyleBox["s", "TI"]], "\[TildeTilde]", 
         FractionBox["4", 
          SubscriptBox["\[Xi]\[Omega]", "0"]]}], TraditionalForm], 
     "errors" -> {}, "input" -> "t_s \\approx \\frac{4}{\\xi \\omega_0}", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "eda28e1a-ed1f-40c4-ada5-f5079c3aa39e"],
 "\nand overshoot ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["M", "TI"], 
          StyleBox["p", "TI"]], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["e", "TI"], 
          FractionBox[
           RowBox[{"-", "\[Pi]\[Xi]"}], 
           RowBox[{"1", "-", 
             SuperscriptBox["\[Xi]", "2"]}]]]}], TraditionalForm], 
     "errors" -> {}, "input" -> "M_p = e^{\\frac{-\\pi\\xi}{ 1-\\xi^2}}", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "80bfb225-f7ef-423e-ba1a-221e708c80d6"],
 "."
}], "Text",
 CellChangeTimes->{{3.9257811870015373`*^9, 3.925781288229616*^9}, {
